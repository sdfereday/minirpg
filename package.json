{
  "name": "parcel-sandbox",
  "version": "1.0.0",
  "description": "Simple Parcel Sandbox",
  "main": "index.html",
  "scripts": {
    "start": "parcel index.html -d build/ --open",
    "build": "parcel build index.html -d build/"
  },
  "dependencies": {
    "kontra": "7.1.2",
    "mithril": "2.0.4",
    "trim-right": "1.0.1"
  },
  "devDependencies": {
    "@babel/core": "7.2.0",
    "parcel-bundler": "^1.6.1"
  },
  "staticFiles": {
    "staticPath": "assets",
    "watcherGlob": "**"
  }
}